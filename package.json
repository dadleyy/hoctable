{
  "name": "hoctable",
  "version": "3.0.2",
  "description": "A library of high order components build with typescript and react.",
  "config": {
    "es5_out": "./dist/es5/hoctable",
    "es6_out": "./dist/es6/hoctable",
    "es5_min_out": "./dist/es5/hoctable/hoctable-umd.min.js",
    "td_out": "./dist/typings/hoctable"
  },
  "scripts": {
    "clean": "rimraf ./dist ./tmp",
    "build:watch": "webpack --config webpack.umd.config.js --watch",
    "build:es5:debug": "webpack --config webpack.umd.config.js",
    "build:es5:minify": "uglifyjs $npm_package_config_es5_out/hoctable-umd.js -o $npm_package_config_es5_min_out",
    "build:es5:release": "npm run build:es5:debug && npm run build:es5:minify",
    "build:typings:gen": "tsc -p ./tsconfig.json -outFile $npm_package_config_td_out/index.js -declaration",
    "build:typings": "npm run build:typings:gen && rimraf $npm_package_config_td_out/index.js",
    "build:es6": "tsc -p ./tsconfig.json -outDir $npm_package_config_es6_out -module es6 -target es6",
    "build:full": "npm run clean && npm run build:es5:release && npm run build:typings && npm run build:es6",
    "types:install": "typings i",
    "example": "webpack-dev-server --config webpack.examples.config.js",
    "test": "karma start ./karma.conf.js --single-run true",
    "test:watch": "karma start ./karma.conf.js --single-run false"
  },
  "author": "Danny Hadley <dadleyy@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-external-helpers": "6.8.0",
    "babel-plugin-transform-es2015-modules-amd": "6.8.0",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-runtime": "6.18.0",
    "bluebird": "3.4.6",
    "dotenv": "2.0.0",
    "express": "4.14.0",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-babel-preprocessor": "6.0.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-requirejs": "1.1.0",
    "karma-typescript-preprocessor": "0.3.0",
    "react-tools": "0.13.3",
    "request": "2.79.0",
    "requirejs": "2.3.2",
    "rimraf": "2.5.4",
    "ts-loader": "1.2.2",
    "typescript": "2.0.10",
    "typings": "2.0.0",
    "uglify-js": "2.7.3",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.15.0"
  }
}
