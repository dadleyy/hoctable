{
  "name": "hoctable",
  "version": "1.0.0",
  "description": "A high order component table for react.",
  "scripts": {
    "compile": "./node_modules/.bin/babel src --out-dir tmp",
    "pack": "./node_modules/.bin/webpack --config webpack.umd.config.js",
    "minify": "./node_modules/.bin/uglifyjs ./dist/hoctable-umd.js -o ./dist/hoctable-umd.min.js",
    "build:debug": "npm run compile && npm run pack",
    "build:release": "npm run build:debug && npm run minify",
    "devserver": "./node_modules/.bin/webpack-dev-server --config webpack.examples.config.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "author": "Danny Hadley <dadleyy@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.5",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "uglify-js": "^2.7.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0",
    "babel-plugin-transform-runtime": "^6.12.0"
  }
}
