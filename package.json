{
  "name": "hoctable",
  "version": "1.0.0",
  "description": "A high order component table for react.",
  "scripts": {
    "clean": "rimraf ./dist ./tmp",
    "minify": "uglifyjs ./dist/hoctable-umd.js -o ./dist/hoctable-umd.min.js",
    "build:watch": "webpack --config webpack.umd.config.js --watch",
    "build:debug": "webpack --config webpack.umd.config.js",
    "build:release": "npm run build:debug && npm run minify",
    "example": "webpack-dev-server --config webpack.examples.config.js",
    "test": "karma start ./karma.conf.js --single-run true",
    "test:watch": "karma start ./karma.conf.js --single-run false"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "author": "Danny Hadley <dadleyy@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.5",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-transform-es2015-modules-amd": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "bluebird": "^3.4.6",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-requirejs": "^1.1.0",
    "react-tools": "^0.13.3",
    "request": "^2.79.0",
    "requirejs": "^2.3.2",
    "rimraf": "^2.5.4",
    "uglify-js": "^2.7.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "dependencies": {
    "dotenv": "^2.0.0"
  }
}
