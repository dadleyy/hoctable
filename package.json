{
  "name": "hoctable",
  "version": "9.0.0",
  "description": "A library of high order components build with typescript and react.",
  "config": {
    "umd_out": "./dist/umd/hoctable",
    "es5_out": "./dist/es5/hoctable",
    "es5_bundle_out": "./dist/es5-bundle/hoctable/hoctable.js",
    "es5_bundle_min_out": "./dist/es5-bundle/hoctable/hoctable.min.js",
    "es6_out": "./dist/es6/hoctable",
    "umd_min_out": "./dist/umd/hoctable/hoctable-umd.min.js",
    "td_out": "./dist/typings/hoctable",
    "ghp_out": "./dist/gh-pages",
    "ghp_css_flags": "-o ./dist/gh-pages/css --include-path ./node_modules/bulma"
  },
  "scripts": {
    "clean": "rimraf ./dist ./tmp",
    "build:watch": "watch \"npm run build:all\" ./src",
    "build:watch:es5": "watch \"npm run clean && npm run build:es5 && npm run build:es5:bundle\" ./src",
    "build:umd:debug": "webpack --config webpack.config.js",
    "build:umd:minify": "uglifyjs $npm_package_config_umd_out/hoctable-umd.js -o $npm_package_config_umd_min_out",
    "build:umd": "npm run build:umd:debug && npm run build:umd:minify",
    "build:es5:b:make": "tsc -p ./tsconfig.json -outFile $npm_package_config_es5_bundle_out -module amd -target es5",
    "build:es5:b:min": "uglifyjs -o $npm_package_config_es5_bundle_min_out $npm_package_config_es5_bundle_out",
    "build:es5:bundle": "npm run build:es5:b:make && npm run build:es5:b:min",
    "build:es5": "tsc -p ./tsconfig.json -outDir $npm_package_config_es5_out -module amd -target es5",
    "build:typings:gen": "tsc -p ./tsconfig.json -outFile $npm_package_config_td_out/index.js -declaration",
    "build:typings": "npm run build:typings:gen && rimraf $npm_package_config_td_out/index.js",
    "build:es6": "tsc -p ./tsconfig.json -outDir $npm_package_config_es6_out -module es6 -target es6",
    "build:browser": "npm run build:umd && npm run build:es5 && npm run build:es6 && npm run build:es5:bundle",
    "build:all": "npm run build:typings && npm run build:browser",
    "build:ghp:js": "tsc -p ./example/ghp/tsconfig.json -outFile $npm_package_config_ghp_out/js/app.js",
    "build:ghp:html": "pug -o $npm_package_config_ghp_out ./example/ghp/*.pug",
    "build:ghp:css": "node-sass ./example/ghp/css/main.sass $npm_package_config_ghp_css_flags",
    "build:ghp:watch": "watch \"npm run build:ghp\" ./example/ghp",
    "build:ghp": "npm run build:ghp:js && npm run build:ghp:html && npm run build:ghp:css",
    "prepublish": "npm run clean && npm run build:all",
    "example:server": "webpack-dev-server --config example/browser/webpack.config.js",
    "test": "karma start ./karma.conf.js --single-run true",
    "test:watch": "karma start ./karma.conf.js --single-run false"
  },
  "author": "Danny Hadley <dadleyy@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.23.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-external-helpers": "6.18.0",
    "babel-plugin-transform-es2015-modules-amd": "6.18.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-runtime": "6.23.0",
    "bluebird": "3.4.7",
    "bulma": "^0.3.0",
    "express": "4.14.1",
    "jasmine-ajax": "^3.2.0",
    "jasmine-core": "2.5.2",
    "karma": "1.5.0",
    "karma-babel-preprocessor": "6.0.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-effroi": "0.0.0",
    "karma-jasmine": "1.1.0",
    "karma-narrow-reporter": "^2.0.1",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-requirejs": "1.1.0",
    "karma-typescript-preprocessor": "0.3.1",
    "node-sass": "^4.0.0",
    "pug": "^2.0.0-beta7",
    "pug-cli": "^1.0.0-alpha6",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-tools": "0.13.3",
    "request": "2.79.0",
    "requirejs": "2.3.3",
    "rimraf": "2.6.1",
    "ts-loader": "2.0.0",
    "typescript": "2.2.1",
    "uglify-js": "2.8.0",
    "watch": "^1.0.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "@types/react": "^15.0.0",
    "@types/react-dom": "^0.14.21",
    "dotenv": "^3.0.0"
  }
}
